<div id="portal-logos">
<a metal:define-macro="portal_logo"
   id="portal-logo"
   tal:attributes="href view/home_url"
   i18n:domain="flbca">
    <img src="logo.gif" alt=""
         tal:replace="structure view/logo_tag" /></a>
<a metal:define-macro="portal_sitelogo"
   id="site-logo"
   accesskey="1"
   tal:attributes="href view/navigation_root_url"
   i18n:domain="flbca">
    <img src="sitelogo.jpg" alt=""
         tal:replace="structure view/sitelogo_tag" /></a>
<a metal:define-macro="portal_sectionlogo"
   id="section-logo"
   tal:condition="view/is_not_home"
   tal:attributes="href view/section_root_url"
   i18n:domain="flbca">
     <img src="sectionlogo.jpg" alt=""
         tal:replace="structure view/sectionlogo_tag" /></a>
</div>
